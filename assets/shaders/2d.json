{
    "type": "Shader",
    "name": "2D/Default",
    "lang": "GLSL",
    "vertex": {
        "input": [
            { "type": "Vec4", "name": "Position" },
            { "type": "Vec4", "name": "UV" }
        ],
        "output": [
            { "type": "Vec4", "name": "Pixel_Position" },
            { "type": "Vec4", "name": "Pixel_UV" }
        ],
        "main": [
            "Pixel_Position = Position",
            "Pixel_UV = UV",
            "gl_Position = Pixel_Position"
        ]
    },
    "pixel": {
        "input": [
            { "type": "Vec4", "name": "Pixel_Position" },
            { "type": "Vec4", "name": "Pixel_UV" }
        ],
        "output": [
            { "type": "Vec4", "name": "Out_Color" }
        ],
        "data": [
            { "type": "Texture2D", "name": "MainTexture" }
        ],
        "main": [
            "Out_Color = texture(MainTexture, Pixel_UV.xy)"
        ]
    }
}