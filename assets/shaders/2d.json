{
    "type": "Shader",
    "name": "2D/Default",
    "lang": "GLSL",
    "vertex": {
        "input": [
            { "type": "vec4", "name": "Attr_Position" },
            { "type": "vec4", "name": "Attr_Texcoord" }
        ],
        "output": [
            { "type": "vec4", "name": "Pixel_Position" },
            { "type": "vec4", "name": "Pixel_Texcoord" }
        ],
        "main": [
            "Pixel_Position = Attr_Position",
            "Pixel_Texcoord = Attr_Texcoord",
            "gl_Position = Pixel_Position"
        ]
    },
    "pixel": {
        "input": [
            { "type": "vec4", "name": "Pixel_Position" },
            { "type": "vec4", "name": "Pixel_Texcoord" }
        ],
        "output": [
            { "type": "vec4", "name": "Out_Color" }
        ],
        "data": [
            { "type": "sampler2D", "name": "MainTexture" }
        ],
        "main": [
            "Out_Color = texture(MainTexture, Pixel_Texcoord.xy)"
        ]
    }
}